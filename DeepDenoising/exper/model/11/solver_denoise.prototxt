train_net: './model/11/train_denoise.prototxt'
#test_net: '<TEST_NET_HOLDER>'
#test_iter: 33
# Carry out testing every 500 training iterations.
#test_interval: 10000
#test_initialization: true
base_lr: 1e-4
lr_policy: "fixed"
#lr_policy: "step"
#gamma: 0.1
#stepsize: 50000
display: 1000
average_loss: 1000
max_iter: 1500000
momentum: 0.9
weight_decay: 0.0005
snapshot: 25000
snapshot_prefix: "./model/11/denoising_net"
solver_mode: GPU
clip_gradients: 100
#debug_info: true
